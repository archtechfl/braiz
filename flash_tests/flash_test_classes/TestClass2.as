package flash_test_classes {		import flash.display.*;	import flash.events.*;	import flash.utils.*;	import flash.net.*;	import flash.geom.Rectangle;		public class TestClass2 extends MovieClip {				private var myLoader:Loader;		private var bitmapData:BitmapData;		private var bmVis:Bitmap;		private var collision:Collision;				public function TestClass2() {			// constructor code					myLoader = new Loader();		myLoader.load(new URLRequest("image_analysis-test_1_image.gif"));		myLoader.contentLoaderInfo.addEventListener(Event.COMPLETE, onComplete);					}				private function onComplete (e:Event):void		{			trace("loaded!");			bitmapData = e.target.content.bitmapData;			bmVis = new Bitmap(bitmapData);			this.addChild(bmVis);			pictureAnalysis();		}				private function pictureAnalysis():void{				for (var j = 0; j < bitmapData.width; j++) {    	for (var k = 0; k < bitmapData.height; k++) {        // Read the pixel color        var color:uint = bitmapData.getPixel(j, k);        // Read color channel values        var alpha:uint = color >> 24 & 0xFF;        var red:uint   = color >> 16 & 0xFF;        var green:uint = color >> 8 & 0xFF;        var blue:uint  = color & 0xFF;				if (color == 16711680){			collision = new Collision();			addChild(collision);			collision.x = j;			collision.y = k;		}        // Reassemble the color		trace(j,k);        trace("color: 0x" + red.toString(16) + green.toString(16) + blue.toString(16));   		}		}				}			}//end of class	}//end of package